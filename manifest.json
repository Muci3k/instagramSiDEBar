{
  "manifest_version": 2,
  "name": "Instagram",
  "version": "1.0",
  "author": "Maciej Muci3k Adach",
  "description": "Instagram umieszczony w sidebarze",
  "sidebar_action": {
    "default_panel": "sidebar.html",
    "browser_style": false,
    "default_icon": {
      "16": "icons/instagram_icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "sidebar.html",
    "default_icon": {
      "16": "icons/m.png",
      "48": "icons/instagram_icon.png",
      "128": "icons/instagram_icon.png"
    }
  },
  "icons": {
    "16": "icons/instagram_icon.png",
    "48": "icons/instagram_icon.png",
    "128": "icons/instagram_icon.png"
  }
}